{
  "name": "mimi",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc && copyfiles -u 1 \"src/**/*.sql\" \"src/**/*.json\" \"src/**/*.md\" \"src/**/*.yml\" \"src/**/*.css\" dist",
    "start": "npm run build && node dist/index.js",
    "dev": "cross-env NODE_ENV=development tsc && node dist/index.js",
    "deploy": "npm run build && node dist/deploy-commands.js",
    "clean-emojis": "npm run build && node dist/scripts/clean-invalid-emojis.js",
    "migrate:transcripts": "npm run build && node dist/scripts/migrate-transcripts-to-r2.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=./test-results.xml"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "@takumi-rs/image-response": "^0.62.6",
    "@types/js-yaml": "^4.0.9",
    "async-retry": "^1.3.3",
    "chalk": "^5.4.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "discord-html-transcripts": "^3.2.0",
    "discord.js": "^14.25.1",
    "dompurify": "^3.2.6",
    "dotenv": "^17.0.1",
    "es-toolkit": "^1.39.8",
    "html-to-image": "^1.11.13",
    "js-yaml": "^4.1.0",
    "jsdom": "^26.1.0",
    "kysely": "^0.28.2",
    "lucide-react": "^0.562.0",
    "marked": "^16.1.2",
    "moment": "^2.30.1",
    "node-fetch": "^3.3.2",
    "openai": "^5.10.1",
    "p-limit": "^6.2.0",
    "pg": "^8.16.3",
    "puppeteer": "^24.12.0",
    "react": "^18.3.1",
    "redis": "^5.7.0",
    "string-similarity": "^4.0.4",
    "typescript": "^5.8.3",
    "winston": "^3.17.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.9",
    "@types/jsdom": "^21.1.7",
    "@types/marked": "^5.0.2",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.15.4",
    "@types/react": "^18.3.27",
    "@types/string-similarity": "^4.0.2",
    "@types/trusted-types": "^2.0.7",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "vitest": "^4.0.16",
    "vitest-mock-extended": "^3.1.0"
  }
}
